{
  "name": "rudolph",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "RudolphApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["rudolph.lib"],
      "main": "TestRudolph",
      "name": "TestRudolph.exe"
    },
    "library": {
      "name": "rudolph.lib",
      "namespace": "Rudolph"
    },
    "executable": {
      "require": ["rudolph.lib"],
      "main": "RudolphApp",
      "name": "RudolphApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestRudolph.exe"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
