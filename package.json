{
  "name": "rudolph",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "RudolphApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "rudolph.lib"
      ],
      "main": "TestRudolph",
      "name": "TestRudolph.exe"
    },
    "library": {
      "name": "rudolph.lib",
      "namespace": "Rudolph",
      "require": [
        "lwt",
        "cohttp",
        "cohttp-lwt-unix"
      ],
      "preprocess": [
        "pps ppx_fast_pipe"
      ]
    },
    "executable": {
      "name": "rudolph.exe",
      "main": "CommandLine",
      "require": [
        "rudolph.lib",
        "cmdliner"
      ],
      "preprocess": [
        "pps ppx_fast_pipe"
      ]
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestRudolph.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/cmdliner": "1.0.3",
    "@opam/cohttp": "1.2.0",
    "@opam/cohttp-lwt-unix": "1.2.0",
    "@opam/dune": ">=1.6.0",
    "@opam/lwt": "4.1.0",
    "@opam/ppx_fast_pipe": "0.0.1",
    "ocaml": "^4.4.0",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
